        <rdfs:comment>Austrian resident with single employer and correct wage tax</rdfs:comment>
        <hasAnnualWageIncome rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">40000.0</hasAnnualWageIncome>
        <hasOtherIncome rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.0</hasOtherIncome>
        <hasSingleEmployer rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</hasSingleEmployer>
        <hasCorrectWageTax rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</hasCorrectWageTax>
        <hasVaryingIncomeNoRollup rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</hasVaryingIncomeNoRollup>
        <hasEmployerChange rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</hasEmployerChange>
        <hasSVRepaymentEligibility rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</hasSVRepaymentEligibility>
        <hasUnclaimedTaxCredits rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</hasUnclaimedTaxCredits>
